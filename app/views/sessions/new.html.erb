<h1>Login</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>: <%= f.text_field :email %><br>
  <%= f.label :password %>: <%= f.password_field :password %><br>
  <%= f.submit "Login" %>
<% end %>

<p><%= link_to "Login with Google", '/auth/google' %></p>
<p>New user? <%= link_to "Sign up!", new_user_path %></p>
