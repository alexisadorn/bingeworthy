<h1><%= current_user.username %>'s Stations</h1>
<% alphebetize(@stations).each do |station| %>
  <p><%= link_to station.name, station_path(station) %></p>
<% end %>

<hr>

<p>Favorite Shows</p>
<% @favorites.each do |show| %>
<p> <%= link_to show.title, show_path(show) %></p>
<% end %>
<button><%= link_to "Create a new Station", new_station_path %></button>
<button><%= link_to "Add Shows", shows_path %></button>
<br>
<br>
<%= link_to "See All of My Shows", user_shows_path(current_user) %></p>
<%= link_to "View My Account Settings", user_path(current_user) %></p>
<%= link_to "Logout", logout_path %></p>
