Watchlist:
<% listings.each do |listing| %>
  <%= link_to listing.watchlist.name, user_watchlist_path(current_user, listing.watchlist) %><br>
  User Status: <%= listing.user_status %><br>
  User Season: <%= listing.user_season %><br>

  <%= link_to "Edit Watchlist Details", edit_show_listing_path(show, listing) %><br>

  <%= form_for listing, method: :delete do |f| %>
    <%= f.submit "Remove This Show from This Watchlist" %>
  <% end %>
  <br>
<% end %>
